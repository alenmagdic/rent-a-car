<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Model" namespace="Model">
  <class name="Model.Vehicle, Model" lazy="false" table="Vehicle">
    <id name="Id" column="Id">
      <generator class="native"/>
    </id>

    <many-to-one name="Brand" class="Model.VehicleBrand, Model" column="Brand"/>
    <property name="ModelName" column="ModelName"/>
    <property name="Category" column="Category"/>
    <property name="TrunkCapacityLiters" column="TrunkCapacityLiters"/>
    <property name="NumberOfSeats" column="NumberOfSeats"/>
    <property name="EngineType" column="EngineType"/>
    <property name="EnginePower" column="EnginePower"/>
    <property name="TransmissionType" column="TransmissionType"/>
    <property name="Length" column="Length"/>
    <property name="Height" column="Height"/>
    <property name="Width" column="Width"/>
    <property name="VehicleId" column="VehicleId"/>
    <property name="LicensePlate" column="LicensePlate"/>
    

  </class>
</hibernate-mapping>